{
  "private": true,
  "scripts": {
    "setup": "supabase start && npm run supabase:genTypes",
    "dev": "next dev",
    "build": "supabase db push --db-url $SUPABASE_DB_URL && next build",
    "build:local": "supabase  db push  --local && next build",
    "start": "next start",
    "lint": "next lint",
    "supabase:genTypes": "supabase gen types --lang typescript --local > types/supabaseTypes.ts",
    "supabase:migrate": "supabase migration up --include-all --local",
    "supabase:local": "supabase start",
    "supabase:reset": "supabase stop && supabase delete && npm run supabase:local",
    "ci:check": "npm run lint && npm run test ",
    "test:prod": "supabase db push --db-url $SUPABASE_TEST_DB_URL && playwright test ",
    "test": "playwright test",
    "test:fast": "CI=true playwright test --workers '70%' --retries 1"
  },
  "engines": {
    "node": ">=20.18.1"
  },
